{
  "name": "hotelier-webchat-ui",
  "version": "1.0.0",
  "description": "UI for Staytus hotelier webchat",
  "main": "index.js",
  "scripts": {
    "compile-styles": "node-sass --output-style expanded --source-map true --source-map-contents true --precision 6 styles.scss dist/styles.css",
    "css-minify": "cleancss --level 1 --format breaksWith=lf --source-map --source-map-inline-sources --output dist/styles.min.css dist/styles.css",
    "process-styles": "npm run compile-styles && npm run css-minify",
    "watch:sass": "npm run build:sass && npm run build:sass -- -w",
    "build:sass": "node-sass -r --output-style compressed src/styles.scss -o dist"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/IrfDev/hotelier-webchat-ui.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/IrfDev/hotelier-webchat-ui/issues"
  },
  "homepage": "https://github.com/IrfDev/hotelier-webchat-ui#readme",
  "devDependencies": {
    "clean-css-cli": "^5.2.0",
    "node-sass": "^5.0.0"
  },
  "dependencies": {
    "sass": "^1.32.8"
  }
}
